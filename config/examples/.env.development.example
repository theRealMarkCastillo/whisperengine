# =======================================================
# WhisperEngine Development Configuration
# Full development features with debugging and hot-reload
# =======================================================

# =======================================================
# DISCORD CONFIGURATION
# =======================================================
DISCORD_BOT_TOKEN=your_discord_bot_token_here
DISCORD_BOT_NAME=WhisperEngine-Dev
ADMIN_USER_IDS=your_discord_user_id_here
DISCORD_MESSAGE_TRACE=true

# =======================================================
# DEVELOPMENT ENVIRONMENT
# =======================================================
ENV_MODE=development
ENVIRONMENT=development
DEBUG_MODE=true
DEV_MODE=true
CONTAINER_MODE=false

# =======================================================
# CHARACTER CONFIGURATION
# =======================================================
CDL_DEFAULT_CHARACTER=characters/examples/elena-rodriguez.json

# =======================================================
# LLM CONFIGURATION (LOCAL DEVELOPMENT)
# =======================================================
LLM_CHAT_API_URL=http://localhost:1234/v1
LLM_CHAT_API_KEY=not-needed
LLM_CHAT_MODEL=local-model
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS_CHAT=4096
LLM_REQUEST_TIMEOUT=90

# =======================================================
# ADVANCED LOGGING (DEVELOPMENT)
# =======================================================
LOG_LEVEL=DEBUG
CONSOLE_LOG_LEVEL=DEBUG
PYTHONUNBUFFERED=1
FORCE_READABLE_LOGS=true

# Performance monitoring
ENABLE_PERFORMANCE_MONITORING=true
PERFORMANCE_LOG_LEVEL=debug

# =======================================================
# MEMORY SYSTEM (VECTOR-NATIVE)
# =======================================================
MEMORY_SYSTEM_TYPE=vector
VECTOR_QDRANT_HOST=localhost
VECTOR_QDRANT_PORT=6333
VECTOR_EMBEDDING_MODEL=snowflake/snowflake-arctic-embed-xs

# Memory debugging
ENABLE_MEMORY_MONITORING=true
MEMORY_OPTIMIZATION_LEVEL=debug

# =======================================================
# DATABASE CONFIGURATION (LOCAL)
# =======================================================
# PostgreSQL
POSTGRESQL_HOST=localhost
POSTGRESQL_PORT=5432
POSTGRESQL_DATABASE=whisper_engine_dev
POSTGRESQL_USERNAME=bot_user
POSTGRESQL_PASSWORD=dev_password_123

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
USE_REDIS_CACHE=true
REDIS_CACHE_TTL_MINUTES=5

# =======================================================
# AI INTELLIGENCE FEATURES (ALL ENABLED)
# =======================================================
ENABLE_EMOTIONAL_INTELLIGENCE=true
ENABLE_PHASE3_MEMORY=true
ENABLE_PHASE4_HUMAN_LIKE=true
ENABLE_PHASE4_THREAD_MANAGER=true
ENABLE_PHASE4_PROACTIVE_ENGAGEMENT=true

# Development AI features
AI_EMOTIONAL_RESONANCE=true
AI_ADAPTIVE_MODE=true
AI_PERSONALITY_ANALYSIS=true
ENABLE_DYNAMIC_PERSONALITY=true

# =======================================================
# PERFORMANCE OPTIMIZATION (DEVELOPMENT)
# =======================================================
ENABLE_PRODUCTION_OPTIMIZATION=true
ENABLE_PARALLEL_PROCESSING=true
AI_MEMORY_OPTIMIZATION=true
CACHE_STRATEGY=adaptive
ENABLE_BACKGROUND_PROCESSING=true

# =======================================================
# VOICE FEATURES (DEVELOPMENT)
# =======================================================
VOICE_SUPPORT_ENABLED=true
VOICE_RESPONSE_ENABLED=true
VOICE_AUTO_JOIN=false
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# =======================================================
# BACKUP & MAINTENANCE
# =======================================================
AUTO_BACKUP_ENABLED=true
AUTO_BACKUP_INTERVAL_HOURS=6
BACKUP_RETENTION_COUNT=10

# =======================================================
# DEVELOPMENT DEBUGGING
# =======================================================
# Multi-entity debugging
MULTI_ENTITY_DEBUG=true
LOG_RELATIONSHIP_CHANGES=true
LOG_INTERACTION_DETAILS=true

# Memory debugging
SEMANTIC_CLUSTERING_TIMEOUT=60
EMBEDDING_MAX_RETRIES=5

# Job scheduler debugging
JOB_SCHEDULER_ENABLED=true
JOB_SCHEDULER_CHECK_INTERVAL_SECONDS=10

# =======================================================
# HEALTH MONITORING
# =======================================================
HEALTH_CHECK_PORT=9090
HEALTH_CHECK_HOST=0.0.0.0