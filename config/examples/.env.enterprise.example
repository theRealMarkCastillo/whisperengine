# =======================================================
# WhisperEngine Enterprise Configuration
# Full enterprise features: multi-entity relationships, advanced AI, monitoring, security
# =======================================================

# üè¢ ENTERPRISE SETUP:
#   1. Copy to .env
#   2. Configure enterprise database cluster
#   3. Set up monitoring infrastructure
#   4. Deploy with production security

# =======================================================
# DISCORD CONFIGURATION (ENTERPRISE)
# =======================================================
DISCORD_BOT_TOKEN=your_enterprise_discord_bot_token_here
DISCORD_BOT_NAME=WhisperEngine-Enterprise
ADMIN_USER_IDS=your_admin_user_ids_comma_separated

# =======================================================
# CHARACTER CONFIGURATION (CDL ENTERPRISE)
# =======================================================
CDL_DEFAULT_CHARACTER=characters/default_assistant.json

# Enterprise character options:
# CDL_DEFAULT_CHARACTER=characters/dream_of_the_endless.json
# CDL_DEFAULT_CHARACTER=characters/examples/elena-rodriguez.json
# CDL_DEFAULT_CHARACTER=characters/examples/marcus-chen.json

# Multi-Entity Character System Features (Enterprise)
ENABLE_MULTI_ENTITY_RELATIONSHIPS=true
ENABLE_CHARACTER_CREATION=true
ENABLE_RELATIONSHIP_EVOLUTION=true
ENABLE_AI_FACILITATED_INTRODUCTIONS=true
ENABLE_CROSS_CHARACTER_AWARENESS=true
ENABLE_CHARACTER_SIMILARITY_MATCHING=true
ENABLE_SOCIAL_NETWORK_ANALYSIS=true

# Character System Limits (Enterprise)
MAX_CHARACTERS_PER_USER=25
MAX_CHARACTER_NAME_LENGTH=100
MAX_CHARACTER_BACKGROUND_LENGTH=2000

# Roleplay System Configuration (Enterprise)
ENABLE_ROLEPLAY_COMMANDS=true
ROLEPLAY_MEMORY_ISOLATION=true
ROLEPLAY_AUTO_CLEAR_ON_SWITCH=true
ROLEPLAY_SESSION_TIMEOUT_MINUTES=240

# =======================================================
# ENTERPRISE LLM CONFIGURATION
# =======================================================
# OpenRouter with premium models
LLM_CHAT_API_URL=https://openrouter.ai/api/v1
LLM_CHAT_API_KEY=your_enterprise_openrouter_api_key_here
LLM_CHAT_MODEL=openai/gpt-4o

# Alternative: OpenAI Enterprise
# LLM_CHAT_API_URL=https://api.openai.com/v1
# LLM_CHAT_API_KEY=your_enterprise_openai_api_key_here
# LLM_CHAT_MODEL=gpt-4o

# =======================================================
# ENTERPRISE MEMORY SYSTEM (VECTOR-NATIVE)
# =======================================================
MEMORY_SYSTEM_TYPE=vector
VECTOR_QDRANT_HOST=qdrant-cluster
VECTOR_QDRANT_PORT=6333
VECTOR_QDRANT_COLLECTION=whisperengine_enterprise
VECTOR_EMBEDDING_MODEL=snowflake/snowflake-arctic-embed-l

# Advanced embedding options for enterprise
# VECTOR_EMBEDDING_MODEL=text-embedding-3-large
# VECTOR_EMBEDDING_MODEL=BAAI/bge-large-en-v1.5

# =======================================================
# PHASE 1 & 2 MEMORY FEATURES (ENTERPRISE)
# =======================================================
# Phase Features Control (All Enterprise Features)
ENABLE_PHASE1_ENHANCED_MEMORY=true
ENABLE_PHASE2_THREE_TIER_MEMORY=true
ENABLE_PHASE3_MEMORY_NETWORKS=true
ENABLE_PHASE4_INTELLIGENCE=true
ENABLE_PHASE4_THREAD_MANAGER=true
ENABLE_PHASE4_PROACTIVE_ENGAGEMENT=true
DISABLE_PHASE2_EMOTION=false
DISABLE_PHASE4_INTELLIGENCE=false

# Phase 1: Enhanced Memory Features (Enterprise Precision)
PHASE1_EMOTIONAL_DETECTION_ENABLED=true
PHASE1_EMOTION_VERY_POSITIVE_THRESHOLD=0.90
PHASE1_EMOTION_POSITIVE_THRESHOLD=0.65
PHASE1_EMOTION_VERY_NEGATIVE_THRESHOLD=0.90
PHASE1_EMOTION_NEGATIVE_THRESHOLD=0.65
PHASE1_EMOTIONAL_TRAJECTORY_ENABLED=true
PHASE1_EMOTIONAL_MOMENTUM_WEIGHT=0.8
PHASE1_SIGNIFICANCE_SCORING_ENABLED=true
PHASE1_SIGNIFICANCE_EMOTIONAL_WEIGHT=0.4
PHASE1_SIGNIFICANCE_CONTEXT_WEIGHT=0.4
PHASE1_SIGNIFICANCE_INTERACTION_WEIGHT=0.2

# Phase 2: Three-Tier Memory & Decay (Enterprise Scale)
PHASE2_THREE_TIER_ENABLED=true
PHASE2_MEMORY_DECAY_ENABLED=true
PHASE2_MEMORY_DECAY_RATE=0.08
PHASE2_TIER_PROMOTE_TO_MEDIUM_SIGNIFICANCE=0.65
PHASE2_TIER_PROMOTE_TO_MEDIUM_MIN_AGE_DAYS=2
PHASE2_TIER_PROMOTE_TO_LONG_SIGNIFICANCE=0.85
PHASE2_TIER_PROMOTE_TO_LONG_MIN_AGE_DAYS=5
PHASE2_MEMORY_DECAY_PROTECTION_THRESHOLD=0.85
PHASE2_MEMORY_DECAY_HIGH_PROTECTION_MULTIPLIER=0.05
PHASE2_MEMORY_DECAY_DELETE_THRESHOLD=0.03
EMOTION_GRAPH_SYNC_INTERVAL=180

# Enterprise Tier-Specific Settings
PHASE2_DECAY_SHORT_TERM_MULTIPLIER=1.0
PHASE2_DECAY_MEDIUM_TERM_MULTIPLIER=0.4
PHASE2_DECAY_LONG_TERM_MULTIPLIER=0.05
PHASE2_MEMORY_TIER_SHORT_TERM_MAX_AGE_DAYS=10
PHASE2_MEMORY_TIER_MEDIUM_TERM_MAX_AGE_DAYS=45

# =======================================================
# ENTERPRISE DATABASE CLUSTER
# =======================================================
# PostgreSQL (enterprise cluster with failover)
POSTGRESQL_HOST=postgres-cluster.enterprise.local
POSTGRESQL_PORT=5432
POSTGRESQL_DATABASE=whisper_engine_enterprise
POSTGRESQL_USERNAME=enterprise_user
POSTGRESQL_PASSWORD=your_enterprise_postgres_password_here

# Redis (enterprise cluster with sentinel)
REDIS_HOST=redis-cluster.enterprise.local
REDIS_PORT=6379
REDIS_PASSWORD=your_enterprise_redis_password_here
REDIS_DB=0

# =======================================================
# ENTERPRISE VOICE SYSTEM
# =======================================================
VOICE_SERVICE_TYPE=discord_elevenlabs

# ElevenLabs Enterprise
ELEVENLABS_API_KEY=your_enterprise_elevenlabs_api_key_here
ELEVENLABS_VOICE_ID=21m00Tcm4TlvDq8ikWAM
ELEVENLABS_MODEL_ID=eleven_multilingual_v2
ELEVENLABS_VOICE_STABILITY=0.5
ELEVENLABS_VOICE_SIMILARITY_BOOST=0.75
ELEVENLABS_VOICE_STYLE=0.0
ELEVENLABS_VOICE_USE_SPEAKER_BOOST=true

# =======================================================
# ADVANCED ENGAGEMENT ENGINE
# =======================================================
ENGAGEMENT_ENGINE_TYPE=full
ENGAGEMENT_ENABLED=true
ENGAGEMENT_CHECK_INTERVAL=180
ENGAGEMENT_MEMORY_THRESHOLD=15
ENGAGEMENT_CONTEXT_WINDOW=10

# =======================================================
# PHASE 4 CONVERSATION INTELLIGENCE (ENTERPRISE)
# =======================================================
PHASE4_INTEGRATION_ENABLED=true
MOMENT_DETECTION_ENABLED=true
EMOTIONAL_INTELLIGENCE_ENABLED=true
PROACTIVE_ENGAGEMENT_ENABLED=true

# Advanced conversation features
CONVERSATION_CONTEXT_LIMIT=25
CONVERSATION_MEMORY_LIMIT=100
CONVERSATION_HISTORY_DAYS=365

# =======================================================
# MULTI-ENTITY RELATIONSHIP SYSTEM
# =======================================================
# Advanced relationship tracking
MULTI_ENTITY_ENABLED=true
RELATIONSHIP_TRACKING_ENABLED=true
ENTITY_MEMORY_PERSISTENCE=true

# Relationship analysis
RELATIONSHIP_ANALYSIS_DEPTH=advanced
SOCIAL_DYNAMICS_TRACKING=true
GROUP_CONVERSATION_ANALYSIS=true

# =======================================================
# ENTERPRISE SECURITY & COMPLIANCE
# =======================================================
# Maximum security
SECURITY_AUDIT_ENABLED=true
PRIVACY_MODE=enterprise
DATA_RETENTION_DAYS=365

# Compliance features
AUDIT_LOGGING_ENABLED=true
ENCRYPTION_AT_REST=true
ENCRYPTION_IN_TRANSIT=true

# Rate limiting (enterprise)
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_BURST_SIZE=25

# =======================================================
# ENTERPRISE MONITORING & OBSERVABILITY
# =======================================================
# Advanced logging
LOG_LEVEL=INFO
CONSOLE_LOG_LEVEL=INFO
FILE_LOG_LEVEL=DEBUG
AUDIT_LOG_LEVEL=INFO

# Enterprise monitoring
PERFORMANCE_MONITORING_ENABLED=true
METRICS_COLLECTION_ENABLED=true
DISTRIBUTED_TRACING_ENABLED=true
HEALTH_CHECK_ENABLED=true
HEALTH_CHECK_PORT=9090

# External monitoring integration
PROMETHEUS_METRICS_ENABLED=true
GRAFANA_DASHBOARD_ENABLED=true
ELASTICSEARCH_LOGGING_ENABLED=true

# =======================================================
# ENTERPRISE PERFORMANCE OPTIMIZATION
# =======================================================
# Production optimizations
HOT_RELOAD_ENABLED=false
AUTO_RESTART_ON_ERROR=true
DEVELOPMENT_MODE=false

# Enterprise performance
PARALLEL_PROCESSING_ENABLED=true
MEMORY_OPTIMIZATION_ENABLED=true
CACHE_STRATEGY=distributed
BACKGROUND_PROCESSING_ENABLED=true

# Advanced AI features
AI_EMOTIONAL_RESONANCE=true
AI_ADAPTIVE_MODE=true
AI_PERSONALITY_ANALYSIS=true
DYNAMIC_PERSONALITY_ENABLED=true

# =======================================================
# ENTERPRISE BACKUP & DISASTER RECOVERY
# =======================================================
# Automated backup
AUTO_BACKUP_ENABLED=true
AUTO_BACKUP_INTERVAL_HOURS=6
BACKUP_RETENTION_COUNT=90

# Disaster recovery
BACKUP_ENCRYPTION_ENABLED=true
OFFSITE_BACKUP_ENABLED=true
POINT_IN_TIME_RECOVERY=true

# =======================================================
# ENTERPRISE FEATURE FLAGS
# =======================================================
# All enterprise features enabled
EXPERIMENTAL_FEATURES_ENABLED=true
BETA_MEMORY_FEATURES=true
BETA_CONVERSATION_FEATURES=true

# Enterprise-only features
ADVANCED_ANALYTICS_ENABLED=true
PREDICTIVE_ENGAGEMENT_ENABLED=true
SENTIMENT_ANALYSIS_ENTERPRISE=true

# =======================================================
# ENTERPRISE INTEGRATION
# =======================================================
# External system integration
API_GATEWAY_ENABLED=true
WEBHOOK_NOTIFICATIONS_ENABLED=true
THIRD_PARTY_INTEGRATIONS_ENABLED=true

# Enterprise protocols
SAML_SSO_ENABLED=false
LDAP_INTEGRATION_ENABLED=false
OAUTH2_ENTERPRISE_ENABLED=false

